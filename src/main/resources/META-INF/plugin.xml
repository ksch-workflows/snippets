<idea-plugin>
    <id>io.github.ksch_workflows.snippets</id>
    <name>K.S.C.H. Workflows Snippets</name>
    <vendor email="info@experimental-software.com" url="https://ksch-workflows.github.io">KS Plus e.V.</vendor>

    <description><![CDATA[
        Dart and Java snippets for the development on the K.S.C.H. Workflows project.
    ]]></description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplates file="/liveTemplates/DartTest.xml"/>
        <defaultLiveTemplates file="/liveTemplates/FlutterTest.xml"/>
        <defaultLiveTemplates file="/liveTemplates/KSCH.xml"/>
        <defaultLiveTemplates file="/liveTemplates/KschDart.xml"/>
        <defaultLiveTemplates file="/liveTemplates/KschFlutter.xml"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>
